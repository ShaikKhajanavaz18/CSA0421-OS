#include <stdio.h>
int main(){
    int block[20]={0}, n, i, size, start, ok=1;
    scanf("%d",&n);
    scanf("%d %d",&start,&size);

    for(i=start;i<start+size;i++)
        if(block[i]) ok=0;

    if(ok){
        for(i=start;i<start+size;i++) block[i]=1;
        printf("Allocated blocks: ");
        for(i=start;i<start+size;i++) printf("%d ", i);
    } else printf("Cannot allocate (Not continuous)");
}
OUTPUT:
Allocated blocks: 5 6 7 8
