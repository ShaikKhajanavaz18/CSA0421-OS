#include <stdio.h>

int main() {
    int b, p, i, j;
    scanf("%d", &b);
    int block[b];
    for(i=0;i<b;i++) scanf("%d", &block[i]);

    scanf("%d", &p);
    int process[p], alloc[p];
    for(i=0;i<p;i++){ scanf("%d", &process[i]); alloc[i] = -1; }

    for(i=0;i<p;i++){
        int idx=-1, best=9999, worst=-1;

        for(j=0;j<b;j++)
            if(block[j] >= process[i]){
                if(block[j] < best) best = block[j];
                if(block[j] > worst) worst = block[j];
                if(idx == -1) idx = j;
            }

        int ff = idx;
        int bf = -1, wf = -1;

        for(j=0;j<b;j++) if(block[j]==best) bf=j;
        for(j=0;j<b;j++) if(block[j]==worst) wf=j;

        if(ff!=-1){ alloc[i]=ff; block[ff]-=process[i]; }
    }

    printf("Process Size Block\n");
    for(i=0;i<p;i++)
        printf("%d\t%d\t%d\n", i+1, process[i], alloc[i]);
    return 0;
}

OUTPUT:
Process Size Block
1       212     1
2       417     4
3       112     1
4       426     4
