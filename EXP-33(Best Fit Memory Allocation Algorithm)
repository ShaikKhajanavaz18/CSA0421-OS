#include <stdio.h>

int main() {
    int b, p, i, j;
    scanf("%d", &b);
    int block[b];
    for(i=0;i<b;i++) scanf("%d", &block[i]);

    scanf("%d", &p);
    int process[p], alloc[p];
    for(i=0;i<p;i++){ scanf("%d", &process[i]); alloc[i] = -1; }

    for(i=0;i<p;i++){
        int bf=-1;
        for(j=0;j<b;j++)
            if(block[j]>=process[i]){
                if(bf==-1 || block[j]<block[bf]) bf=j;
            }
        if(bf!=-1){ alloc[i]=bf; block[bf]-=process[i]; }
    }

    printf("Process Size Block\n");
    for(i=0;i<p;i++)
        printf("%d\t%d\t%d\n", i+1, process[i], alloc[i]);
    return 0;
}

OUTPUT:
Process Size Block
1       212     3
2       417     4
3       112     0
4       426     4
